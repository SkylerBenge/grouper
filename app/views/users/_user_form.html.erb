<%= form_for @user do |f| %>
  <br/>
  <br/>
  <%= f.label :email %>
  <br/>
  <%= f.text_field :email %>
  <br/>
  <br/>
  <%= f.label :password, "Password" %>
  <br/>
  <%= f.password_field :password %>
  <br/>
  <br/>
  <%= f.label :password_confirmation, "Confirm Passowrd" %>
  <br/>
  <%= f.password_field :password_confirmation %>
  <br/>
  <%= f.submit("Submit") %>
<% end %>
